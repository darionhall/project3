<!DOCTYPE html>
<html lang="en">
<head>
<link rel="stylesheet" type="text/css" href="/stylesheets/index.css">
  <% include ../partials/head %>
</head>

<body class="container-fluid">
  <header>
    <% include ../partials/header %>
  </header>

  <main>
    <div>
      <h3>styles: <a href="/styles/new" class="btn btn-primary">New</a></h3>
      <% styles.forEach(function(style) { %>
        <h4>
          <form method="POST" action="/styles/<%= style._id %>?_method=DELETE" class="stylebox">
            <a href="/styles/<%= style._id %>">
            </a>
            <img src="<%= style.avatar_url" alt="">
            <button type="submit" class="btn btn-xs btn-danger">Delete</button>
          </form>
        </h4>
      <% }) %>
    </div>
  </main>

  <footer>
    <% include ../partials/footer %>
  </footer>
</body>
</html>
